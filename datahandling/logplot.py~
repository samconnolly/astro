#===============================================================================
# Simple, generic plotting programme to plot any two columns from an ascii data
# table, with errors, ignoring any header, as a log plot
#===============================================================================

# Import packages

import numpy as np
import pylab as plt

x = np.arange(0.1,100,0.1)
y = np.power(x,2)

fig = plt.figure()
ax = fig.add_subplot(1,1,1)
ax.set_yscale('log')
ax.set_xscale('log')

plt.plot(x,y)

plt.show()





